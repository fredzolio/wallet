# Database
POSTGRES_DB=wallet
POSTGRES_USER=wallet
POSTGRES_PASSWORD=wallet_password
DATABASE_URL=postgresql+asyncpg://wallet:wallet_password@db:5432/wallet

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# pgAdmin
PGADMIN_DEFAULT_EMAIL=admin@email.com
PGADMIN_DEFAULT_PASSWORD=admin_password

# Keycloak
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin_password
KC_DB=postgres
KC_DB_URL=jdbc:postgresql://db:5432/wallet
KC_DB_USERNAME=wallet
KC_DB_PASSWORD=wallet_password
KC_FEATURES=preview
KEYCLOAK_URL=http://keycloak:8080
KEYCLOAK_REALM=wallet
KEYCLOAK_CLIENT_ID=wallet-api
KEYCLOAK_CLIENT_SECRET=
USE_KEYCLOAK=true

# Grafana
GF_SECURITY_ADMIN_USER=admin
GF_SECURITY_ADMIN_PASSWORD=grafana_password
GF_USERS_ALLOW_SIGN_UP=false

# API
SECRET_KEY=
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7
ALGORITHM=HS256

# Configurações Gerais
API_V1_STR=/api/v1
PROJECT_NAME=Wallet API

# CORS
CORS_ORIGINS=http://localhost:8000,http://localhost:3000

# OAuth2
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
OAUTH_CALLBACK_URL=http://localhost:8000/api/v1/auth/google/callback

# MFA
MFA_ISSUER=RioWallet

# Chatbot
GOOGLE_GENAI_API_KEY=
GOOGLE_GENAI_MODEL=gemini-2.0-flash
USE_LLM=true
ENABLE_PROMETHEUS=true